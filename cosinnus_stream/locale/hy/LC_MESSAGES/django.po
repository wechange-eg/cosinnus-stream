msgstr "Խմբագրել այս հոսքը"

msgid "Delete this Stream"
msgstr "Ջնջել այս հոսքը"

msgid "Do you really want to delete this stream?"
msgstr "Իսկապե՞ս ցանկանում եք ջնջել այս հոսքը?"

msgid "Delete Stream"
msgstr "Ջնջել հոսքը"

msgid "Create Stream"
msgstr "Ստեղծել հոսք"

